{
  "name": "@foxglove/eslint-config",
  "version": "0.1.1",
  "description": "Foxglove base eslint configuration",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:prettier": "eslint -c prettier.js .",
    "lint:react": "eslint -c react.js .",
    "lint:typescript": "eslint -c typescript.js .",
    "lint:all": "eslint -c prettier.js -c react.js -c typescript.js ."
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4",
    "eslint-config-prettier": "^8",
    "eslint-plugin-file-progress": "^1",
    "eslint-plugin-import": "^2",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^4"
  },
  "peerDependencies": {
    "eslint": "^7",
    "prettier": "^2"
  },
  "devDependencies": {
    "typescript": "^4"
  }
}
